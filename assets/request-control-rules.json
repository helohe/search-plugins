  {
    "uuid": "d78b46f3-6c04-43a8-a907-83c161138e61",
    "active": true,
    "pattern": {
      "scheme": "*",
      "host": [
        "www.linkedin.com"
      ],
      "path": [
        "*"
      ],
      "excludes": [
        "/\\/checkpoint\\//",
        "/\\/jobs\\/\\?/",
