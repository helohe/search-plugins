        "/\\/search\\/\\?/",
        "/\\/uas\\/login\\?/",
        "/\\/uas\\/logout\\?/"
      ]
    },
    "types": [
      "main_frame",
      "sub_frame"
    ],
    "action": "filter",
    "paramsFilter": {
      "values": [
        "url"
      ],
      "invert": true
    }
