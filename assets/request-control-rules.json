[
  {
    "title": "Anonymize pictures from Google Maps and Google Street View",
    "description": "May cause problems in some browsers like Firefox ESR 68. Disabled by default.",
    "uuid": "6d25126a-dd1d-4e63-874c-fe0592e9bec1",
    "active": false,
    "pattern": {
      "scheme": "*",
      "host": [
        "*.ggpht.com",
        "*.google.com",
        "*.googleapis.com"
      ],
      "path": [
        "cbk?*",
        "kh?*",
        "maps/vt?*",
        "v1/tile*"
      ]
    },
    "types": [
      "image"
    ],
    "action": "filter",
    "paramsFilter": {
      "values": [
        "cb_client",
        "center",
        "deg",
        "fov",
        "h",
        "heading",
        "hl",
        "key",
        "location",
        "maptype",
        "markers",
        "output",
        "panoid",
        "pb",
        "pitch",
        "radius",
        "signature",
        "size",
        "source",
        "thumb",
        "thumbfov",
        "v",
        "w",
        "x",
        "y",
        "yaw",
        "z",
        "zoom"
      ],
      "invert": true
    }
  },
